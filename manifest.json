{
    "manifest_version": 3,
    "name": "Scalesbot Antipink",
    "description": "This extension will create a force field that annihilate pink background color from the Scalesbot web app for good.",
    "version": "1.0",
    "host_permissions": ["<all_urls>"],
    "content_scripts": [
        {
            "matches": ["*://*.scalesbot.com/*"],
            "js": ["inject.js"]
        }
    ],
    "icons": {
        "16": "/images/16.png",
        "32": "/images/32.png",
        "48": "/images/48.png",
        "128": "/images/128.png"
    }
  }
  